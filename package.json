{
  "name": "formik-project",
  "private": true,
  "devDependencies": {
    "@changesets/changelog-github": "^0.2.7",
    "@changesets/cli": "^2.10.3",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "cypress": "^5.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "got": "10.0.4",
    "husky": "^4.3.0",
    "lerna": "^3.19.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "release": "6.0.1",
    "tsdx": "^0.14.1",
    "typescript": "^4.0.3"
  },
  "resolutions": {
    "**/typescript": "^4.0.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "0.0.0-experimental-27659559e",
    "react-dom": "0.0.0-experimental-27659559e"
  },
  "workspaces": [
    "packages/*",
    "packages/@formik/*",
    "app/*"
  ],
  "scripts": {
    "lerna": "lerna",
    "dev": "yarn --cwd ./app/context-selector run start",
    "dev:subscriptions": "yarn --cwd ./app/subscriptions run start",
    "dev:mutable-source": "yarn --cwd ./app/mutable-source run start",
    "test": "lerna run test --",
    "test:watch": "lerna run test --stream --no-sort -- --watch",
    "test:watchAll": "lerna run test --stream --no-sort -- --watchAll",
    "test:vscode": "lerna run test --stream --no-sort --",
    "build": "lerna run build",
    "prepublish": "lerna run prepublish",
    "format": "prettier --write 'examples/**/*' 'packages/*/src/**/*' 'docs/src/**/*.{ts,tsx,js,jsx,md,mdx}' 'app/*/pages/**/*'  'packages/*/test/**/*' 'README.md'",
    "changeset": "changeset",
    "release": "changeset publish",
    "git-reset": "git reset --hard HEAD",
    "git-clean": "git clean -d -x -e node_modules -e packages -f",
    "cypress": "cypress run",
    "precommit": "lint-staged",
    "cypress:open": "cypress open",
    "start:app": "yarn --cwd ./app/context-selector run start",
    "unlink": "yarn --cwd node_modules/react unlink && yarn --cwd packages/formik unlink"
  },
  "lint-staged": {
    "**/*.{ts,tsx,md,mdx,js,jsx}": [
      "npm run format",
      "tsdx lint"
    ]
  },
  "dependencies": {
    "cross-env": "^7.0.3"
  }
}
